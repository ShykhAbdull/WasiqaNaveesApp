<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <solid android:color="#FFFFFF" /> <!-- Inner circle color (white) -->
    <size
        android:width="20dp"
        android:height="20dp" />
    <stroke
        android:dashGap=".8dp"
        android:dashWidth="4dp"
        android:width="3dp"
        android:color="#000000" /> <!-- Border color -->
</shape>
